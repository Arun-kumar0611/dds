#include<iostream>
using namespace std;
int main(){
    int arr[]={10,2,5,4,7};
    int n=sizeof(arr)/sizeof(arr[0]);
    for(int i=0;i<n;i++){
        for(int i=0;i<n-1;i++){
            if(arr[i]>arr[i+1]){
                swap(arr[i],arr[i+1]);
            }
        }
        for(int i=0;i<n;i++){
            cout<<arr[i]<<" ";
        }
    }
}
